<?xml version="1.0"?>

<!DOCTYPE faces-config PUBLIC
   "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
   "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<faces-config>
   <navigation-rule>
      <from-view-id>/index.jsp</from-view-id>
         <navigation-case>
            <to-view-id>/welcome.jsp</to-view-id>
         </navigation-case>
   </navigation-rule>

   <component>
      <component-type>com.corejsf.TabbedPane</component-type>
      <component-class>javax.faces.component.UICommand</component-class>
   </component>

   <render-kit>
      <renderer>
         <component-family>javax.faces.Command</component-family>
         <renderer-type>com.corejsf.TabbedPane</renderer-type>
         <renderer-class>com.corejsf.TabbedPaneRenderer</renderer-class>
      </renderer>
   </render-kit>
</faces-config>
